@page "/Calendar/{Month=DateTime.Now.Month}/{Year=DateTime.Now.Year}"

@using DodoPlanner.Components


@model DodoPlanner.Pages.CalendarModel
    <script>
    function getcookie(name)
    {
        const value = `; ${document.cookie}`;
        const parts = value.split(`; ${name}=`);
        if (parts.length === 2) return parts.pop().split(';').shift();
    }
    </script>
@(await Html.RenderComponentAsync<Calendar>(RenderMode.ServerPrerendered, new { Month = @RouteData.Values["Month"], Year = @RouteData.Values["Year"] }))
